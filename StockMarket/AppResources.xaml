<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:StockMarket">

    <!--Colors-->
    
    <!-- usually used for negative return-->
    <Color x:Key="PaleRed" R="255" G="127" B="127" A="255" />
    <!-- usually used for positive return-->
    <Color x:Key="PaleGreen" R="222" G="255" B="209" A="255"/>
    <!-- usually used for dividends-->
    <Color x:Key="PaleBlue" R="112" G="200" B="255" A="255"/>
    <!-- usually used for when everything is sold-->
    <Color x:Key="LightGray" R="155" G="155" B="155" A="255"/>
    <Color x:Key="DarkGray" R="100" G="100" B="100" A="255"/>

</ResourceDictionary>